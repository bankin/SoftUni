	public static char generateCard(int value){
		String card;
		switch(value){
			case 2:
			case 3:
			case 4:
			case 5:
			case 6:
			case 7: 
			case 8:
			case 9: card = Integer.toString(value);
			case 10: card = "T"; break;
			case 11: card = "J"; break;
			case 12: card = "Q"; break;
			case 13: card = "K"; break;
			case 14: card = "A"; break;
		}
		return card[0];
	}
	
